<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
    <object class="tk.Toplevel" id="toplevel1">
        <property name="height">200</property>
        <property name="title" translatable="yes">Py Image Resizer</property>
        <property name="width">200</property>
        <child>
            <object class="ttk.Frame" id="frame1">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                    <property name="propagate">True</property>
                    <property name="side">top</property>
                </layout>
                <child>
                    <object class="ttk.Labelframe" id="dropzone">
                        <property name="height">200</property>
                        <property name="text" translatable="yes">Drag your files here:</property>
                        <property name="width">500</property>
                        <layout manager="pack">
                            <property name="propagate">True</property>
                            <property name="side">top</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Frame" id="frame3">
                        <property name="height">200</property>
                        <property name="width">200</property>
                        <layout manager="pack">
                            <property name="padx">10</property>
                            <property name="pady">10</property>
                            <property name="propagate">True</property>
                            <property name="side">right</property>
                        </layout>
                        <child>
                            <object class="ttk.Label" id="label3">
                                <property name="text" translatable="yes">Resolution:</property>
                                <layout manager="pack">
                                    <property name="propagate">True</property>
                                    <property name="side">left</property>
                                </layout>
                            </object>
                        </child>
                        <child>
                            <object class="ttk.Combobox" id="dpiComboSel">
                                <layout manager="pack">
                                    <property name="propagate">True</property>
                                    <property name="side">right</property>
                                </layout>
                            </object>
                        </child>
                    </object>
                    <object class="ttk.Frame" id="frame4">
                        <property name="height">200</property>
                        <property name="width">200</property>
                        <layout manager="pack">
                            <property name="padx">10</property>
                            <property name="pady">10</property>
                            <property name="propagate">True</property>
                            <property name="side">right</property>
                        </layout>
                        <child>
                            <object class="ttk.Label" id="label4">
                                <property name="text" translatable="yes">Scale so that... </property>
                                <layout manager="pack">
                                    <property name="propagate">True</property>
                                    <property name="side">left</property>
                                </layout>
                            </object>
                        </child>
                        <child>
                            <object class="ttk.Combobox" id="boundsComboSel">
                                <layout manager="pack">
                                    <property name="propagate">True</property>
                                    <property name="side">right</property>
                                </layout>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="ttk.Frame" id="frame6">
                <property name="height">200</property>
                <property name="width">500</property>
                <layout manager="pack">
                    <property name="expand">true</property>
                    <property name="fill">both</property>
                    <property name="propagate">True</property>
                    <property name="side">bottom</property>
                </layout>
                <child>
                    <object class="ttk.Scrollbar" id="yscrollbar">
                        <property name="orient">vertical</property>
                        <layout manager="pack">
                            <property name="anchor">center</property>
                            <property name="expand">true</property>
                            <property name="fill">y</property>
                            <property name="pady">10</property>
                            <property name="propagate">True</property>
                            <property name="side">right</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="tk.Text" id="console">
                        <property name="background">#000000</property>
                        <property name="foreground">#00ff00</property>
                        <property name="height">10</property>
                        <property name="width">58</property>
                        <layout manager="pack">
                            <property name="padx">5</property>
                            <property name="pady">10</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
